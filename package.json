    "cloudinary": "^1.26.2",
    "concurrently": "^6.2.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "swagger-ui-express": "^4.1.6"
